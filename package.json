{
  "name": "iban-utils",
  "description": "Tool for validating IBANs & extracting detailed bank and branch information.",
  "version": "0.1.2",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "homepage": "https://github.com/leszekkorzan/iban-utils",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leszekkorzan/iban-utils.git"
  },
  "bugs": {
    "url": "https://github.com/kefranabg/readme-md-generator/issues"
  },
  "keywords": [
    "IBAN",
    "IBAN validation",
    "IBAN extractor",
    "Poland IBAN",
    "bank info from IBAN",
    "nodejs IBAN package",
    "validate IBAN",
    "IBAN node",
    "polish iban",
    "polish bank number"
  ],
  "files": [
    "/dist"
  ],
  "scripts": {
    "start": "tsc",
    "build": "tsup",
    "biome:ci": "npx biome ci .",
    "biome:fix": "npx @biomejs/biome format --write && npx @biomejs/biome lint --write",
    "release": "npm run build && changeset publish",
    "test": "npm run biome:ci && jest"
  },
  "author": "leszekkorzan",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "@changesets/cli": "^2.27.8",
    "@types/jest": "^29.5.13",
    "jest": "^29.7.0",
    "terser": "^5.34.1",
    "ts-jest": "^29.2.5",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2"
  }
}
